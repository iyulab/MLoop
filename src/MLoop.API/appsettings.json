{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "MLoop-Default-Secret-Key-Change-In-Production-Min-32-Chars",
    "Issuer": "MLoop.API",
    "Audience": "MLoop.Client"
  }
}
